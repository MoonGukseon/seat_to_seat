
<div class="row">
            <% @place.each do |x| %>
                <div class="col-md-4 col-sm-4">
                    <div class="thumbnail">
                        <a href="/info/<%= x.id %>">
                        <% if x.image.blank? %>
                        <img src="/assets/defaultimg.jpg" alt="">
                        <% else %>
                        <img src="<%= x.image %>"  style="width: 300px; height: 200px;">
                        <% end %>
                            <div class="caption">
                                <%= x.region %><br>
                                <%= x.place_name %>
                            </div>
                        </a>
                    </div>
                </div>
             <% end %>
        </div>


</div>